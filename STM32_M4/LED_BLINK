#include <stdint.h>
#include "stm32f446ff.h"


int main(void)
{
	GPIOA_CLK_EN(); // Enable GPIO Clock

	/* Output = 01 */
	GPIOA->MODER &= ~(0x3 << (5 * 2)); // Clear
	GPIOA->MODER |= (1 << 10); // Set

	/* Set  Output push-pull */
	GPIOA->OTYPER &= ~(1 << 5);

	/* Set speed medium */
	GPIOA->OSPEEDR &= ~(0x3 << (5 * 2));
	GPIOA->OSPEEDR |= (1 << 10);

	/* No pull-up/pull-down */
	GPIOA->PUPDR &= ~(0x3 << (5 * 2));

	/* Turn LED Blink */
	while(1)
	{
	GPIOA->ODR ^= ( 1 << 5);  // Toggle LED
	for(volatile int i=0; i<=500000; i++); // Delay
	}
}
